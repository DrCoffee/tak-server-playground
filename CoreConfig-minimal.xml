<?xml version="1.0" encoding="UTF-8"?>
<Configuration xmlns="http://bbn.com/marti/xml/config">
    <network multicastTTL="5">
        <input _name="stdtcp" protocol="tcp" port="8087" auth="anonymous"/>
        
        <!-- web connectors - only HTTP -->
        <connector port="8080" tls="false" _name="http_plaintext"/>
    </network>
    
    <auth>
        <File location="UserAuthenticationFile.xml"/>
    </auth>
    
    <submission ignoreStaleMessages="false" validateXml="false"/>
    
    <subscription reloadPersistent="false"/>
    
    <repository enable="true" numDbConnections="4">
        <connection url="jdbc:postgresql://localhost:5432/cot" username="tak" password="tak123" />
    </repository>
    
    <dissemination smartRetry="false" />
    
    <filter>
        <flowtag enable="false" text=""/>
    </filter>
    
    <buffer>
        <latestSA enable="false"/>
    </buffer>
    
    <security>
        <tls context="TLSv1.2"
            keymanager="SunX509"
            keystore="JKS" keystoreFile="files/certs/takserver.jks" keystorePass="atakatak"
            truststore="JKS" truststoreFile="files/certs/takserver.jks" truststorePass="atakatak">
        </tls>
    </security>
</Configuration>